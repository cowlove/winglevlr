    "version": "0.2.0",
    "configurations": [
        {
            "name": "winglevlr",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/winglevlr_ubuntu",
            "args": ["--servovis", "0", "--testStick"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/winglevlr_ubuntu",
            "args": ["--serial","--startpos","47.59509212379994,-122.38743386638778,1000","--seconds","600"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}